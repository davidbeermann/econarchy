<?xml version="1.0" encoding="UTF-8" ?>
<econarchy_controller id="Controller (Xbox 360 Wireless Receiver for Windows)">
	<inputs>
		<!-- buttons defined here -->
		<buttons>
			<button id="1" map_to="jump"/>
			<!-- button id="2" map_to="use"/ -->
		</buttons>
		<!-- analog sticks defined here -->
		<sliders> 
			<slider id="1" map_to="horizontal">
				<tolerance value="0.2" />
				<max value="1.0"/>
				<min value="-1.0"/>
			</slider>
		</sliders>
	</inputs>
	<meta-data>
		<meta-entry type="rumble_enabled" value="true"/>
	</meta-data>
</econarchy_controller>